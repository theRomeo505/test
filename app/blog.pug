extends layout
block page
	.content.content-blog
		include burger-menu

		include header
			block main-header-text
				h1.main-header-text-blog Блог
			block sec-header-text
				h6.sec-header-text-blog Статьи,которые я написал
			block arrow
				img(src='images/arrow_down.svg').arrow
				img(src='images/arrow_down.png').arrow2
				
		main.blog
			.blog-l-tr
				svg.blog-triangle-l(viewbox="0 0 1400 200" preserveAspectRatio="none" fill='#faf8f0')
					polygon(points='0,0 0,200 1500,200')
			.blog-r-tr
				svg.blog-triangle-r(viewbox="100 0 1400 200" preserveAspectRatio="none" fill='#faf8f0')
					polygon(points='0,200 1500,200 1500,0')
			
			a(href='').mobile-button
			ul.blog-sidebar
				li.blog-sd-i.sdactive
